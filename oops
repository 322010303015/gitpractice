class car
 {
   String carbrand;
   String carenginetype;
   String carfueltype;
   int carseatingcapacity;
   String carmodel;
   car(String carbrand,String carenginetype,String carfueltype,int carseatingcapacity, String carmodel)
   {
      
      this.carbrand=carbrand;
      this.carenginetype=carenginetype;
      this.carfueltype=carfueltype;
      this.carseatingcapacity=carseatingcapacity;
      this.carmodel=carmodel;

   }
   void details(){
       System.out.println("Brand:"+carbrand+"\nEngine_type:"+carenginetype+"\n Fueltype:"+carfueltype+"\nSeatingcapacity:"+carseatingcapacity+"\nModel:"+carmodel);
   }
 }
    
